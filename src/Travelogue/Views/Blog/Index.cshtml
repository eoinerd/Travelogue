@model IEnumerable<Travelogue.Models.Blog>
@{
    ViewData["Title"] = "About";
}
<aside>
    <div class="form-group">
        <div id="content"></div>
    </div>
</aside>
<div>
    <div class="well">
        @foreach (var item in Model)
        {
            <div class="row">
                <h3>
                    <a asp-controller="Blog" asp-action="Details" asp-route-id="@item.Id">
                        @Html.DisplayFor(modelItem => item.Title)
                    </a>
                </h3>
            </div>
            <div class="row">
                <h5>@Html.DisplayFor(modelItem => item.Subtitle)</h5>
            </div>
        }
    </div>
    <a class="btn btn-primary" asp-area="" asp-controller="Blog" asp-action="Create">Create Blog</a>
</div>
